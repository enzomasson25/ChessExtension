{
    "manifest_version": 2,
    "name": "Chess Extension",
    "version": "0.1",
    "content_scripts": [
      {
      "matches": ["<all_urls>"],
      "js": [
        "js/script.js"
      ]
      }
    ],
    "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "icons": {
      "16": "images/icon.png",
      "32": "images/icon.png",
      "48": "images/icon.png",
      "128":"images/icon.png"
    }
}

